<simulation>
	<parameters>
		<type>particle_level_set</type>
		<gridSize>100</gridSize>
		<frames>180</frames>
		<domain>0 1</domain>
		<dt>0.0166666666667</dt>
		<maxParticles>35</maxParticles>
	</parameters>
	<levelset>
		<center>0.33 0.33 0.33</center>
		<radius>0.15</radius>
	</levelset>
	<meta reseed='false' />
	<logfile>pls_log35.csv</logfile>
	<baseFolder>results/particles/3d/enright/pls35no/</baseFolder>
	<meshFolder>results/particles/3d/enright/pls35no/mesh/</meshFolder>
</simulation>

